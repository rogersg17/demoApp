{
  "name": "demoapp",
  "version": "1.0.0",
  "description": "Demo web application with user management and Playwright tests",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:production": "./deployment/start-production.sh",
    "dev": "nodemon --exec ts-node server.ts",
    "dev:full": "./start-dev.sh",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:production": "npm run build:frontend && npm run build",
    "build:frontend": "cd frontend && npm run build",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "tsx prisma/seed.ts",
    "deploy": "docker-compose -f deployment/docker-compose.yml up -d",
    "deploy:build": "docker-compose -f deployment/docker-compose.yml build",
    "deploy:logs": "docker-compose -f deployment/docker-compose.yml logs -f",
    "deploy:stop": "docker-compose -f deployment/docker-compose.yml down",
    "deploy:restart": "docker-compose -f deployment/docker-compose.yml restart",
    "health": "curl -f http://localhost:3000/api/health",
    "monitor": "./deployment/monitor-health.sh",
    "migrate": "./deployment/migrate-database.sh",
    "validate:deployment": "./deployment/validate-deployment.sh",
    "test": "npm run test:e2e",
    "test:all": "npm run test:unit && npm run test:e2e && npm run test:api && npm run test:integration && npm run test:validation",
    "test:e2e": "playwright test tests/e2e",
    "test:e2e:headed": "playwright test tests/e2e --headed",
    "test:e2e:ui": "playwright test tests/e2e --ui",
    "test:e2e:static": "USE_SERVER=false playwright test tests/e2e",
    "test:e2e:report": "playwright show-report",
    "test:e2e:login": "VITE_API_BASE_URL=http://localhost:3000 playwright test tests/e2e/login-functional.spec.ts --reporter=line",
    "test:api": "node tests/api/test-api.js && node tests/api/test-api-tests.js && node tests/api/test-api-quick.js",
    "test:api:auth": "node tests/api/test-auth-flow.js",
    "test:api:duration": "node tests/api/test-duration-fix.js",
    "test:integration": "node tests/integration/test-websocket-simple.js && node tests/integration/test-realtime-monitoring.js",
    "test:integration:websocket": "node tests/integration/test-websocket-simple.js && node tests/integration/test-websocket-monitoring.js",
    "test:integration:workflows": "node tests/integration/test-workflow.js && node tests/integration/test-phase1.js",
    "test:validation": "node tests/validation/validate-phase1.js",
    "test:validation:security": "node tests/api/test-security.js && node tests/api/test-auth-flow.js && node tests/api/test-invalid-login.js",
    "test:validation:data": "node tests/validation/validate-week6.js",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "test:performance": "echo 'Performance tests not yet implemented. See tests/performance/README.md for setup instructions.'",
    "test:json": "playwright test tests/e2e --reporter=json --output-dir=./test-results",
    "test:json:compact": "playwright test tests/e2e --reporter=json | jq -c '{stats: .stats, errors: .errors}'",
    "test:summary": "playwright test tests/e2e --reporter=list",
    "test:app": "playwright test tests/e2e --reporter=line --quiet",
    "test:status": "node scripts/test-status.js",
    "test:simple": "playwright test tests/e2e --reporter=line 2>&1 | findstr /C:\"passed\" /C:\"failed\"",
    "test:login": "playwright test tests/e2e/login-*.spec.ts",
    "test:login:headed": "playwright test tests/e2e/login-*.spec.ts --headed",
    "test:demo": "playwright test tests/e2e/jira-demo.spec.ts",
    "test:demo:headed": "playwright test tests/e2e/jira-demo.spec.ts --headed",
    "test:jira": "playwright test tests/e2e --config=playwright.config.jira.ts",
    "test:jira:headed": "playwright test tests/e2e --headed --config=playwright.config.jira.ts",
    "test:jira:ui": "playwright test tests/e2e --ui --config=playwright.config.jira.ts",
    "test:jira:static": "USE_SERVER=false playwright test tests/e2e --config=playwright.config.jira.ts",
    "test:jira:login-functional": "playwright test tests/e2e/login-functional.spec.ts --config=playwright.config.jira.ts",
    "test:jira:login-functional:headed": "playwright test tests/e2e/login-functional.spec.ts --headed --config=playwright.config.jira.ts",
    "test-jira-setup": "node scripts/test-jira-integration.js",
    "setup-jira": "node scripts/setup-jira-integration.js"
  },
  "keywords": [
    "demo",
    "express",
    "webapp",
    "user-management",
    "playwright",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/types": "^30.0.5",
    "@playwright/test": "^1.54.1",
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.1.0",
    "@types/session-file-store": "^1.2.5",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.6.1",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "start-server-and-test": "^2.0.3",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "azure-devops-node-api": "^12.5.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.7.0",
    "prisma": "^6.13.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sqlite3": "^5.1.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "xss": "^1.0.14",
    "yamljs": "^0.3.0"
  }
}
