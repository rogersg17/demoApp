{
  "name": "demoapp",
  "version": "1.0.0",
  "description": "Demo web application with user management and Playwright tests",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "playwright test --config=playwright.config.localhost.js",
    "test:headed": "playwright test --config=playwright.config.localhost.js --headed",
    "test:ui": "playwright test --config=playwright.config.localhost.js --ui",
    "test:report": "playwright show-report"
  },
  "keywords": [
    "demo",
    "express",
    "webapp",
    "user-management",
    "playwright",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "start-server-and-test": "^2.0.3"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
}
